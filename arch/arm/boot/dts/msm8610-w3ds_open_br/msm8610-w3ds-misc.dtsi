&soc {
	earjack-debugger {
		status = "disable";
	};

	i2c@f9924000 {
		rt8542@39 {
			status = "disable";
		};
        	lm3530@38 {
            		status = "ok";
            		revision = "rev_0";
        	};
		lp5521@32 {
            		status = "disable";
            		revision = "rev_0";
		};
	};

    i2c@0 {
        lm3530@38 {
            status = "ok";
            revision = "rev_a...";
        };
        lp5521@32 {
            status = "disable";
            revision = "rev_a...";
        };
    };
};
