&spmi_bus {
	qcom,pm8226@0 {
		qcom,power-on@800{
			qcom,pon_1 {
				revision = "...rev_c";
			};
			qcom,pon_1_revd {
				qcom,pon-type = <0>;
				qcom,pull-up = <1>;
				linux,code = <116>;
				revision = "rev_d...";
				qcom,support-reset = <0>;
			};
			qcom,pon_2 {
				revision = "...rev_c";
				status = "disable";
			};
			qcom,pon_2_revd {
				qcom,pon-type = <1>;
				qcom,pull-up = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <2000>;
				qcom,s2-type = <7>;
				status = "ok";
				revision = "rev_d...";
				qcom,support-reset = <0>;
			};
		};
		/* e9 model does not use leds so a100, a300, a500 will be set to disable */
		qcom,leds@a100 {
			status = "disable";
		};

		qcom,leds@a300 {
			status = "disable";
		};

		qcom,leds@a500 {
			status = "disable";
		};

		qcom,charger {
			qcom,usb-chgpth@1300 {
				status = "okay";
			};
		};
	};
	qcom,pm8226@1 {
		qcom,vibrator@c000 {
                        status = "okay";
                        qcom,vib-timeout-ms = <15000>;
                        qcom,vib-vtg-level-mV = <3100>;
                };

		/* e9 model does not use leds so d300, d800 will be set to disable */
		qcom,leds@d300 {
			status = "disable";
		};

		qcom,leds@d800 {
			status = "disable";
		};
	};
};

&pm8226_gpios {
	/* GPIO 1 : PMIC_AUDIO_REF_CLK */
	/* XO_PMIC_CDC_MCLK enable for tapan codec */
	/* Digital output /CMOS logic / QPNP_PIN_PULL_NO / QPNP_PIN_VIN2 / QPNP_PIN_OUT_STRENGTH_HIGH /QPNP_PIN_SEL_FUNC_1 /Enable GPIO */
	gpio@c000 {
		qcom,mode = <1>;
		qcom,output-type = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <3>;
		qcom,out-strength = <3>;
		qcom,src-sel = <2>;
		qcom,master-en = <1>;
	};

	/* GPIO 2 : EXT_OVP_CTRL */
	gpio@c100 {
		reg = <0xc100 0x100>;
		qcom,pin-num = <2>;
		status = "okay";
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <3>;		/* QPNP_PIN_VIN2 */
		qcom,invert = <0>;		/* QPNP_PIN_INVERT_DISABLE */
		qcom,out-strength = <2>;	/* QPNP_PIN_OUT_STRENGTH_MED */
		qcom,src-sel = <0>;			/* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,master-en = <1>;		/* Enable GPIO */
	};

	/* Rev 0 */
	/* GPIO 2 : Reserved Pin */
	/* QPNP_PIN_MODE_DIG_IN /QPNP_PIN_PULL_DOWN / Disable GPIO */
	/* gpio@c100 { */
	/*	status = "okay"; */
	/*	revision = "rev_0"; */

	/*	qcom,mode = <0>; */
	/*	qcom,pull = <4>; */
	/*	qcom,master-en = <0>; */
	/* }; */

	/* W7 model does not use external OVP except for TMO_US, so GPIO_2 will be set to a reserved pin */
	/* Rev A ... */
	/* GPIO 2 : Reserved Pin */
	/* QPNP_PIN_MODE_DIG_IN /QPNP_PIN_PULL_DOWN / Disable GPIO */
	/* gpio@c101 { */
	/*	reg = <0xc100 0x100>; */
	/*	qcom,pin-num = <2>; */
	/*	status = "okay"; */
	/*	revision = "rev_a..."; */

	/*	qcom,mode = <0>; */
	/*	qcom,pull = <4>; */
	/*	qcom,master-en = <0>; */
	/* }; */

	/* If you want to enable external OVP, use below GPIO_2 setting */
	/* Rev 0 */
	/* GPIO 2 : EXT_OVP_CTRL */
	/* gpio@c100 { */
	/*	reg = <0xc100 0x100>; */
	/*	qcom,pin-num = <2>; */
	/*	status = "okay"; */
	/*	revision = "rev_a"; */
	/*	qcom,mode = <1>; */		/* QPNP_PIN_MODE_DIG_OUT */
	/*	qcom,output-type = <0>; */	/* CMOS logic */
	/*	qcom,pull = <5>; */		/* QPNP_PIN_PULL_NO */
	/*	qcom,vin-sel = <3>; */		/* QPNP_PIN_VIN2 */
	/*	qcom,invert = <0>; */		/* QPNP_PIN_INVERT_DISABLE */
	/*	qcom,out-strength = <2>; */	/* QPNP_PIN_OUT_STRENGTH_MED */
	/*	qcom,src-sel = <0>; */		/* QPNP_PIN_SEL_FUNC_CONSTANT */
	/*	qcom,master-en = <1>; */	/* Enable GPIO */
	/*}; */

	/* GPIO 3 : NFC_CLK_REQ */
	/* QPNP_PIN_MODE_DIG_IN / QPNP_PIN_PULL_DN - Not Fixed / QPNP_PIN_VIN2 / QPNP_PIN_SEL_FUNC_1 */
	gpio@c200 {
		qcom,mode = <0>;
		qcom,pull = <4>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <2>;
		qcom,master-en = <0>;
	};

	/* GPIO 4 : BATT_UIM_ALARM : Reserved Pin */
	gpio@c300 {
	};

	/* GPIO 5 : FLASH_STROBE_TRIG : Reserved Pin */
	/* QPNP_PIN_MODE_DIG_IN / QPNP_PIN_PULL_DN - Not Fixed / QPNP_PIN_VIN2 / QPNP_PIN_SEL_FUNC_1 */
	gpio@c400 {
		status = "okay";

		qcom,mode = <0>;
		qcom,pull = <4>;
		qcom,master-en = <0>;
	};

	/* GPIO 6 : CABC_WLED_PM : Reserved Pin */
	/* QPNP_PIN_MODE_DIG_IN / QPNP_PIN_PULL_NO / QPNP_PIN_VIN2 / QPNP_PIN_SEL_FUNC_1 */
	gpio@c500 {
		status = "okay";

		qcom,mode = <0>;
		qcom,pull = <4>;
		qcom,master-en = <0>;
	};

	/* GPIO 7 : TX_GTR_THRES : Reserved Pin */
	gpio@c600 {
	};

	/* GPIO 8 : BOOST_BYP_BYP */
	/* QPNP_PIN_MODE_DIG_OUT / QPNP_PIN_OUT_BUF_CMOS / QPNP_PIN_PULL_NO / QPNP_PIN_VIN2 */
	/* QPNP_PIN_INVERT_ENABLE / QPNP_PIN_OUT_STRENGTH_MED / QPNP_PIN_SEL_FUNC_CONSTANT / Enable GPIO */
	gpio@c700 {
		qcom,mode = <1>;
		qcom,output-type = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <3>;
		qcom,invert = <1>;
		qcom,out-strength = <2>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
	};
};

&pm8226_mpps {
	/* MPP 1 : VREF_MSM_PX */
	/* QPNP_PIN_MODE_AOUT / QPNP_PIN_AOUT_1V25 - Not Fixe / QPNP_PIN_SEL_FUNC_CONSTANT - QCT TBD / Enable MPP */
	mpp@a000 {
		qcom,mode = <5>;
		qcom,aout-ref = <0>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
	};

	/* Rev 0 */
	/* MPP 2 : HR_LED_SINK : Reserved Pin */
	/* SINK / 5mA / CS Disable */
	mpp@a100 {
		status = "okay";

		qcom,mode = <6>;
		qcom,cs-out = <0>;
		qcom,master-en = <0>;
	};

	/* Rev A ... */
	/* MPP 2 : Reserved Pin */
	/* SINK / 5mA / CS Disable */
