	/*	qcom,vin-sel = <2>; */		/* QPNP_PIN_VIN2 */
	/*	qcom,src-select = <0>; */	/* QPNP_PIN_SEL_FUNC_CONSTANT */
	/*	qcom,master-en = <1>; */
	/* }; */

	/* MPP 5 / PA_THERM0 */
	/* PA_THERM0 config */
	/* AIN input / Enable MPP / AMUX 5 / Master Enable / Function constant */
	mpp@a400 {
		qcom,mode = <4>;
		qcom,invert = <1>;
		qcom,ain-route = <0>;
		qcom,master-en = <1>;
		qcom,src-sel = <0>;
	};

	/* Rev 0 */
