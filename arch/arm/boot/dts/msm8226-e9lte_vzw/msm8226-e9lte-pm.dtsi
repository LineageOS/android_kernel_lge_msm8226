	mpp@a500 {
		status = "okay";

		qcom,mode = <6>;
		qcom,cs-out = <0>;
		qcom,master-en = <0>;
	};

	/* Rev A ... */
	/* MPP 6 : EAR_MIC_JACK */
	/* SINK / 5mA / CS Disable */
	/* mpp@a501 { */
	/* 	reg = <0xa500 0x100>; */
