	 /* MPP 7 : BOARD_ID */
	 /* AIN input / QPNP_PIN_AIN_AMUX_CH7 / QPNP_PIN_AIN_AMUX_CH7 / QPNP_PIN_SEL_FUNC_CONSTANT /Enable MPP */
	mpp@a600 {
		qcom,mode = <4>;
