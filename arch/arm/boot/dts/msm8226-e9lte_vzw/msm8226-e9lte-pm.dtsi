	mpp@a200 {
		qcom,mode = <5>;
		qcom,aout-ref = <0>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
	};

	/* Rev 0 */
	/* MPP 4 : Reserved Pin */
	/* SINK / 5mA / CS Disable */
	mpp@a300 {
		status = "okay";

		qcom,mode = <6>;
		qcom,cs-out = <0>;
		qcom,master-en = <0>;
	};

	/* Rev A ... */
